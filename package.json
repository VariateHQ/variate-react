{
  "name": "@variate/react",
  "version": "3.0.0",
  "description": "Variate React Plugin",
  "license": "BSL 1.1",
  "author": "Thomas Davis <tomjdavis27@gmail.com>",
  "main": "./dist/variate-react.min.js",
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint ./src --ext .ts,.tsx,.jsx,.js",
    "start": "webpack-dev-server --mode development --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@variate/engine": "^3.0.2",
    "react": "^16.12.0"
  },
  "peerDependencies": {
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-stage-2": "^7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.16",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^3.4.0",
    "cz-conventional-changelog": "^3.0.2",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.7.2",
    "eslint-plugin-react": "^7.17.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint": "^0.7.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "standard-version": "^7.0.1",
    "style-loader": "^1.0.2",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jestPreprocessor.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "keywords": [
    "ab-testing",
    "variate"
  ],
  "bugs": {
    "url": "https://github.com/variateapp/variate-react/issues"
  },
  "homepage": "https://github.com/variateapp/variate-react#README.md"
}
